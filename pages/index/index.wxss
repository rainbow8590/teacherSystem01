/**index.wxss**/
.container {
  background-origin: no-repeat; 
  background-size: cover;
  overflow: hidden;
  height: 100vh;
  padding: 0;
  display: block;
}
.page-login .logo {padding: 80rpx 0 90rpx 0;text-align: center;}
.page-login .logo image {width:250rpx;height: 250rpx}
.page-login .login-section {width: 70%;margin: 0 auto;}
.page-login .login-input {background: rgba(255,255,255,.4);border-radius: 30px;padding: 0 10px;margin-bottom: 15px;}
.page-login .login-input .input {background: none; border:0; color:#fff;height: 45px; width:100%;font-size:14px}
.page-login .placeholderC { color:#fff;}
.page-login .login-input:before {  background-position: 0 13px;content: "";display: inline-block; background-size: 18px auto; width: 25px; height: 40px;}
.page-login .login-input:nth-child(2):before {background-position: 0 -18px}
.page-login .radio {color: #ffffff;text-align: center;}
.page-login .radio label {padding: 0 10px;}
.page-login .button .btn {margin-top: 20px;background: none;border:1px solid #fff;border-radius:30px;color: #fff}
.page-login .regLink {left: 0;bottom: 0;width: 100%;position: absolute;text-align: center;color: #b7d3ea;font-size: 12px}
.page-login .regLink navigator {padding: 20px }

.page-login .login-input:before {
  background-repeat: no-repeat;
background-image:url(data:image/png;base64,) ;}
.container {background-image: url(data:image/jpeg;base64,); }