}
/*SideNav*/
.menuIco {background: url(data:image/png;base64,) no-repeat 50% 50%;background-size:25px auto;width: 50rpx; height: 50rpx; position: fixed;right: 40rpx; top: 30rpx}
.mainMenu{ background-size: cover;	background: url(data:image/jpeg;base64,) no-repeat 100% 0;width:55%;position:fixed;right:0;top:0;height:100%;box-shadow:0 0 15px #6fa3c8;z-index:1001;}
.mainMenu .con{border-bottom:1px solid #d7e7f2;margin: 0 30px;position: relative;}
.mainMenu .con navigator:before {
    content: "";
    display:block;
    position:absolute;
    right:5px;
    top:20px;
    border-right: 1px solid #fff; 
    border-bottom: 1px solid #fff; 
    width:8px; 
    height: 8px; 
    transform: rotate(-45deg); 
   
}
.mainMenu .con navigator {color:#fff;display:block;text-indent:10px;font-size:.875rem;padding: 15px 0;}
.mainMenu .btn {background:#abcbeb;text-indent: 0;padding: 0;width: 260rpx;display: inline-block;margin-right: 40px;}
.drawer_screen{background:rgba(0, 0, 0, .5);width:100%;height:100%;position:fixed;left:0;top:0;z-index:1000;}
.userinfo {clear: both;overflow: hidden;padding:50px 0 20px 0;}
.userinfo .Avatar {text-align: center;width: 85px;height: 85px;margin: 5px auto;background: #5c7c97;border-radius: 50%;}
.userinfo .Avatar image {width:50px;margin-top: 15px;}
.userinfo .name {color: #fff;line-height: 24px; font-size: 16px;text-align: center}